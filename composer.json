{
    "name": "yeashy/compliance",
    "description": "Pipeline Validation For Laravel Apps",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0 || ^11.0",
        "illuminate/pipeline": "^10.0 || ^11.0",
        "illuminate/foundation": "^10.0 || ^11.0",
        "illuminate/validation": "^10.0 || ^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Yeashy\\Compliance\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yeashy\\Compliance\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Mikhail Butakov",
            "email": "yeashy.work@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
